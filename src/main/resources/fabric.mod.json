{
  "schemaVersion": 1,
  "id": "afix",
  "version": "${version}",
  "name": "AFix",
  "description": "A mod to fix your 'Ctrl' key becoming stuck after pressing 'Alt Gr', causing 'a' being pressed to delete your entire input",
  "authors": [
    "Dwarslooper"
  ],
  "contact": {
    "repo": "https://github.com/Dwarslooper/AFix",
    "issues": "https://github.com/Dwarslooper/AFix/issues"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/afix/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.dwarslooper.mods.afix.AFixClient"
    ]
  },
  "mixins": [
    "afix.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
